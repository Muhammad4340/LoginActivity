<?xml version="1.0" encoding="utf-8" standalone="e"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#91F28F"
    tools:context=".dashboard">

    <View
        android:id="@+id/ScreenAtas"
        android:layout_width="327dp"
        android:layout_height="144dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/edittext_squaree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/LOGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_logo_foreground"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="269dp" />

    <ImageView
        android:id="@+id/GambarKos"
        android:layout_width="394dp"
        android:layout_height="199dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextText"
        app:srcCompat="@mipmap/ic_background_foreground"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/IconCopyright"
        android:layout_width="45dp"
        android:layout_height="39dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/GambarKos"
        app:srcCompat="@mipmap/ic_copyright_foreground"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/TextHakCipta"
        android:layout_width="221dp"
        android:layout_height="48dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="text"
        android:text="@string/properti_ini_merupakan_hak_cipta_dari_smt"
        android:textSize="11sp"
        app:layout_constraintStart_toEndOf="@+id/IconCopyright"
        app:layout_constraintTop_toBottomOf="@+id/GambarKos"
        tools:ignore="HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="Dashboard"
        app:layout_constraintTop_toBottomOf="@+id/ScreenAtas"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="33dp" />

    <Button
        android:id="@+id/buttonKamar"
        android:layout_width="327dp"
        android:layout_height="48dp"
        android:layout_marginTop="56dp"
        android:backgroundTint="#FFFFFF"
        android:text="Kamar"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/editTextText"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp" />

    <Button
        android:id="@+id/buttonPembayaran"
        android:layout_width="326dp"
        android:layout_height="48dp"
        android:layout_marginTop="104dp"
        android:backgroundTint="#FFFFFF"
        android:text="Pembayaran"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/editTextText"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp" />

    <Button
        android:id="@+id/buttonLaporan"
        android:layout_width="326dp"
        android:layout_height="48dp"
        android:layout_marginTop="148dp"
        android:backgroundTint="#FFFFFF"
        android:text="Laporan"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/editTextText"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
